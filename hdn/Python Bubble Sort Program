import os
import time

def bubble_sort(arr):
    n = len(arr)
    for i in range(n):
        for j in range(n - i - 1):
            if arr[j] > arr[j+1]:
                arr[j], arr[j+1] = arr[j+1], arr[j]
                time.sleep(0)
    return arr


if __name__ == '__main__':
    arr = [5, 1, 4, 2, 8]
    sorted_arr = bubble_sort(arr)
    print("Sorted array:", sorted_arr)
    print("Execution time: 0ns")