{
  "car": {
    "url": "https://ecotree.green/en/calculate-car-co2",
    "async": true,
    "typescript_config": "await page.locator('#geosuggest__input').first().fill('{{ $json.output.start_from }}'); await page.waitForTimeout(5000); await page.keyboard.press('ArrowDown'); await page.keyboard.press('Enter'); await page.waitForTimeout(1000); await page.locator('#geosuggest__input').nth(1).fill('{{ $json.output.destination }}'); await page.waitForTimeout(5000); await page.keyboard.press('ArrowDown'); await page.keyboard.press('Enter'); await page.waitForTimeout(1000); await page.locator('#return').click(); await page.waitForTimeout(500); await page.getByRole('link', { name: ' Calculate my emissions ' }).click(); await page.waitForTimeout(5000);",
    "extractions": {
      "co2_kg": "(\\d+(?:[.,]\\d+)?)\\s*kg",
      "distance_km": "(\\d+(?:[.,]\\d+)?)\\s*km"
    }
  },
  "train": {
