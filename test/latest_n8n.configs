    "typescript_config": "await page.locator('.geosuggest').first().locator('input').fill('{{ $json.output.start_from }}'); await page.waitForTimeout(2000); await page.locator('.geosuggest').first().locator('.geosuggest__item').first().click(); await page.waitForTimeout(1000); await page.locator('.geosuggest').nth(1).locator('input').fill('{{ $json.output.destination }}'); await page.waitForTimeout(2000); await page.locator('.geosuggest').nth(1).locator('.geosuggest__item').first().click(); await page.waitForTimeout(1000); await page.locator('#return').click(); await page.waitForTimeout(500); await page.getByText('{{ $json.output.train_type[0] }}').click(); await page.waitForTimeout(500); await page.getByRole('link', { name: ' Calculate my emissions ' }).click(); await page.waitForTimeout(5000);",
    "extractions": {
      "co2_kg": "(\\d+(?:[.,]\\d+)?)\\s*kg",
      "distance_km": "(\\d+(?:[.,]\\d+)?)\\s*km"
    }
  },
  "plane": {
